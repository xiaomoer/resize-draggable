{
  "name": "resize-draggable",
  "version": "1.0.1",
  "description": "A React component that can be used for sizing by dragging",
  "homepage": "https://github.com/xiaomoer/resize-draggable",
  "bugs": "https://github.com/xiaomoer/resize-draggable/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaomoer/resize-draggable"
  },
  "keywords": [
    "resize",
    "drag",
    "react-draggable",
    "draggable"
  ],
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "demo": "webpack --config demo.config.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/*.js"
    ],
    "testMatch": [
      "<rootDir>/tests/*.js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.less$": "<rootDir>/tests/__mocks__/style.js"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-draggable": "^3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sinon": "^7.4.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  }
}
